<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>AdminLTE 3 | Blank Page</title>

  <!-- Google Font: Source Sans Pro -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="../../plugins/fontawesome-free/css/all.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="~/adminlte.css">
  <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">

</head>
<body class="hold-transition sidebar-mini" style="background-color: #f8f9fd">
<!-- Site wrapper -->
<div class="wrapper">
  <!-- Navbar -->
  
  <!--Navbar common code-->
  @Html.Partial("~/Views/common/navbar.cshtml")

  <!-- /.navbar -->
  <!--sidebar common code-->
    @Html.Partial("~/Views/common/sidebar.cshtml")
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-12 text-center">
            <img src="~/ii.jpg" />
          </div><!-- /.col -->
         
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <div class="row">
          <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-info">
              <div class="inner">
               <a class="nav-link"style="color:white; font-size:30px; font-family:Algerian" asp-area="" asp-controller="login" asp-action="Add_category"> Add Catogery</a>

                <p> &nbsp;</p>
              </div>
              <div class="icon" >
                <i class="ion ion-bag"></i>
              </div>
              <a href="#" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
            </div>
          </div>
          <!-- ./col -->
          <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-success">
              <div class="inner">
              <a class="nav-link"style="color:White; font-size:30px; font-family:Algerian" asp-area="" asp-controller="login" asp-action="viewcategory"> View Catogery</a>

                <p>&nbsp;</p>
              </div>
              <div class="icon">
                <i class="ion ion-stats-bars"></i>
              </div>
              <a href="#" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
            </div>
          </div>
          <!-- ./col -->
          <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box" style="background-color:#f35587">
              <div class="inner">
              <a class="nav-link"style="color:white;font-size:30px;  font-family:Algerian;font-weight:normal" asp-area="" asp-controller="login" asp-action="Add_Question"> Add Question</a>

                <p>&nbsp;</p>
              </div>
              <div class="icon">
                <i class="ion ion-person-add"></i>
              </div>
              <a href="#" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
            </div>
          </div>
          <!-- ./col -->
          <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-danger">
              <div class="inner">
                <a class="nav-link"style="color:white;font-size:30px; font-family:Algerian" asp-area="" asp-controller="login" asp-action="v_Question"> View Questions</a>

                <p>&nbsp;</p>
              </div>
              <div class="icon">
                <i class="ion ion-pie-graph"></i>
              </div>
              <a href="#" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
            </div>
          </div>
           <div class="col-lg-3 col-6" style="text-align: center">
                <div class="clearfix"></div>
           <div class="small-box bg-warning">
              <div class="inner">
               <a class="nav-link"style="color:white;font-size:30px; font-family:Algerian; text-align:left" asp-area="" asp-controller="login" asp-action="Add_subject"> Add_subject</a>

                <p>&nbsp;</p>
              </div>
              <div class="icon">
                <i class="ion ion-person-add"></i>
              </div>
              <a href="#" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
            </div>
          </div>
          <!-- ./col -->
        </div>
        <!-- /.row -->
        <!-- Main row -->
        <div class="row">
          <!-- Left col -->
          <section class="col-lg-7 connectedSortable">
            <!-- /.card -->
            </section>
            </div>
            </section>
            </div>
